{% extends 'admin/base.html.twig' %}

{% block title %}Página não encontrada{% endblock %}

{% block stylesheets %}
    {% embed 'admin/_partials/_block_stylesheets.html.twig' %}
        {% block page_level_styles %}
            <link href="{{ asset('admin/assets/pages/css/error.min.css') }}" rel="stylesheet" type="text/css"/>
        {% endblock %}
    {% endembed %}
{% endblock %}

{% block body %}

    <body class=" page-404-full-page">

    <div class="row">
        <div class="row">
            <div class="col-md-12 page-404">
                <div class="number font-red"> 404</div>
                <div class="details">
                    <h3>Oops! Página não encontrada.</h3>
                    <p> Não podemos encontrar o conteúdo que você está procurando</p>
                </div>
            </div>
        </div>
    </div>
    </body>

    {% block javascripts %}{% endblock %}

{% endblock %}
